<Window x:Class="R423.Page.StartScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:R423.Page"
        mc:Ignorable="d"
        Title="Тропосферная цифровая станция Р-423" Height="600" Width="900">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="60*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <Image Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Source="/Resources/top.jpg" />
        <Image Grid.Row="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Source="/Resources/startScreen.png" />
        <Button Grid.Row="2" Content="Старт!" HorizontalAlignment="Center" VerticalAlignment="Center" Width="100" Height="30" FontSize="18" Click="Button_Click" />
    </Grid>
</Window>
